  "tokenColors": [
    // General
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5C6370"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage",
        "variable.language.super",
        "variable.language.this",
        "variable.language.special"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "String",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "keyword.other.unit",
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "punctuation.definition.decorator",
        "punctuation.decorator",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Type",
      "scope": [
        "meta.type.annotation",
        "meta.type.parameters",
        "support.type.primitive",
        "support.type.builtin",
        "support.type.exception"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "punctuation.accessor",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.assignment",
        "keyword.operator.relational",
        "keyword.operator.ternary",
        "keyword.operator.arithmetic",
        "keyword.operator.rest",
        "keyword.operator.spread",
        "keyword.operator.type",
        "keyword.operator.optional",
        "keyword.operator.combinator",
        "keyword.operator.pattern",
        "keyword.operator.or",
        "keyword.operator.quantifier",
        "keyword.operator.bitwise"
      ],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "keyword.operator.type.annotation",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "entity.name", "support.variable"],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Object key",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#98C379"
      }
    },

    // C++
    {
      "name": "C++ Keywords",
      "scope": [
        "punctuation.definition.directive.cpp",
        "keyword.other.using.directive.cpp"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "C++ Scopes",
      "scope": [
        "entity.name.scope-resolution.parameter.cpp",
        "entity.name.scope-resolution.cpp",
        "entity.name.scope-resolution.function.call.cpp",
        "entity.name.scope-resolution.template.call.cpp",
        "entity.name.scope-resolution.function.definition.cpp"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },

    // CSS/SASS
    {
      "name": "CSS/SASS keyword",
