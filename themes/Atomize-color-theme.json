      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Decorators",
      "scope": ["punctuation.decorator", "punctuation.definition.decorator"],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "Function, Special Method",
